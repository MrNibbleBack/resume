<header>
    {{#with page_header}}
    <div class="text-center">
        <h1 class="text-center"> {{title}} </h1>
        <br>
        <p class="text-uppercase"> {{description}} </p>
        {{/with}}
    </div>
</header>

{{#with section}}
<div class="card">
    <div class="card-body">
        {{#with about_us}}
        <div>
            {{#with title_list}}
            <div class="display-6 text-end"> {{main}} </div>
            <br>
            <h2 class="text-end"> {{sub}} </h2>
            {{/with}}
            <br>
            <button class="btn btn-outline-info"> {{button.text}} </button>
        </div>
        {{/with}}
    </div>
</div>
<br>
<div class="card">
    <div class="card-body">
        {{#with advantage}}
        <div class="display-5"> {{title}} </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with efficiency}}
                <div class="display-4"> {{title}} </div>
                <br>
                <h6> {{sub_title}} </h6>
                <br>
                <p> {{description}} </p>
                <br>
                <button class="btn btn-outline-warning"> {{button}} </button>
                {{/with}}
            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with innovation}}
                <div class="display-4 text-center"> {{title}} </div>
                <br>
                <h6 class="text-center"> {{sub_title}} </h6>
                <br>
                <p class="text-center"> {{description}} </p>
                <br>
                <button class="btn btn-outline-warning"> {{button}} </button>
                {{/with}}
            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with collaboration}}
                <div class="display-4 text-end"> {{title}} </div>
                <br>
                <h6 class="text-end"> {{sub_title}} </h6>
                <br>
                <p class="text-end"> {{description}} </p>
                <br>
                <button class="btn btn-outline-warning"> {{button}} </button>
                {{/with}}
            </div>
        </div>
    </div>
    {{/with}}
</div>
<br>
<div class="card">
    <div class="card-body">
        {{#with agreement}}
        <div class="display-5"> {{title}} </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with info}}
                <h5> {{sub_title}} </h5>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with detail}}
                        <p> {{text}} </p>
                        <br>
                        <button class="btn btn-success"> {{button}} </button>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </div>
</div>


{{/with}}